{
  "name": "Manga Yabu Premium",
  "description": "Manga Yabu Premium",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [],
  "page_action": {
    "default_title": "Manga Yabu Premium",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "js": ["main.js"],
      "matches": ["https://mangayabu.top/*"]
    }
  ]
}
